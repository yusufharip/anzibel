server {
	listen      443 ssl;
	server_name hompimpah.xyz;
	
	ssl_certificate /etc/letsencrypt/live/hompimpah.xyz/fullchain.pem;
	ssl_certificate_key /etc/letsencrypt/live/hompimpah.xyz/privkey.pem;

	location / {
        root   /usr/share/nginx/html;
        index  index.html index.htm;
    }
}

server {
	listen 80 ;
	listen [::]:80;

	server_name hompimpah.xyz;
	return 301 https://$host$request_uri;
}

server {
    listen       9090;
    server_name  localhost;

    location / {
        proxy_pass   http://www.stealmylogin.com;
    }
}